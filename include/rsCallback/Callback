#ifndef RSCALLBACK_CALLBACK_
#define RSCALLBACK_CALLBACK_

#include <rsScene/Scene>
#include <rsSim/Sim>

namespace rsCallback {

	// class
	class LIBRSEXPORT Callback {
		// public functions
		public:
			Callback(void) {};
			virtual ~Callback(void) {};

			void attachCallback(rsScene::Obstacle*, rsSim::Obstacle*);
	};

} // namespace rsCallback

#endif // RSCALLBACK_CALLBACK_

